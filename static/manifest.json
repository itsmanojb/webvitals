{
  "manifest_version": 2,
  "name": "Webvitals",
  "version": "0.0.1",
  "description": "Measure web-vitals (performance metrics) of any website.",
  "default_locale": "en",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "author": "Manoj Barman",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Webvitals"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' http://localhost:8080 'unsafe-eval'; object-src 'self'",
  "permissions": []
}